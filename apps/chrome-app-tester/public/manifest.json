{
  "manifest_version": 3,
  "name": "Side panel APP",
  "version": "1.0",
  "description": "Extension with a default side panel.",
  "icons": {
    "16": "icons/web-software-labs.png"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "permissions": [
    "sidePanel",
    "activeTab",
    "scripting",
    "offscreen",
    "cookies",
    "storage",
    "power",
    "alarms"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "workers/background.js"
  },

  "default_locale": "en",
  "action": {
    "default_title": "Click Me"
  }
}
